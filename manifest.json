{
    "manifest_version": 2,

    "name": "Autolike",
    "description": "Like button on steroids - automate likes in Tinder™, Mamba™ and other services",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "https://*.tinder.com/", "https://*.mamba.ru/"]
}
